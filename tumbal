local httpRequest = http.request or httprequest
if httpRequest then
  httpRequest({
    Url = Webhook,
    Method = 'POST',
    Headers = {
      ['Content-Type'] = 'application/json'
    },
    Body = game:GetService('HttpService'):JSONEncode(data)
  })
else
  print("LMAOOOOOO")
